    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "# 创建必要目录\n",
        "!mkdir -p /content/v5/data/expert_knowledge\n",
        "!mkdir -p /content/v5/logs\n",
        "!mkdir -p /content/v5/checkpoints\n",
        "\n",
        "# 执行阶段1\n",
        "!python trainers/train_two_stage.py --config configs/v5_config.yaml --stage 1\n",
        "\n",
        "# 查看提取的规则\n",
        "import json\n",
        "import os\n",
        "\n",
        "rules_path = '/content/v5/data/expert_knowledge/expert_rules.json'\n",
        "if os.path.exists(rules_path):\n",
        "    with open(rules_path, 'r') as f:\n",
        "        rules = json.load(f)\n",
        "    print(f'✅ 成功提取 {len(rules)} 条专家规则')\n",
        "    print(f'📁 规则文件路径: {rules_path}')\n",
        "    print('\\n📋 前5条专家规则：\\n')\n",
        "    for i, rule in enumerate(rules[:5], 1):\n",
        "        print(f'{i}. {rule[\"title\"]} ({rule[\"category\"]})')\n",
        "        print(f'   内容: {rule[\"content\"][:100]}...')\n",
        "        print(f'   关键点: {len(rule[\"key_points\"])} 个')\n",
        "else:\n",
        "    print('❌ 规则文件不存在，请先执行阶段1')\n"
      ]
